	<!-- 分页查询 -->
	<select id="findAllFy" resultType="TAllmsg"
		parameterType="com.zlkj.visitor.dto.PulicCxtj">
		select * FROM 
		(select * from entryinfo.t_allmsg
		UNION
		select * from entryinfo.t_htallmsg) t
		
		<where>
			<if test="bh==0">
				and yxjzrq <![CDATA[<=]]> #{dqsj} 
			</if>
			<if test="bh==1">
				and yxjzrq >= #{dqsj} 
			</if>
			<if test="bqffqk==0">
				and bqid is not null
			</if>
			<if test="bqffqk==1">
				and bqid is null
			</if>
			<if test="hpzl!=null and hpzl!=''">
				and hpzl in 
	            <foreach collection="hpzl" item="hpzl" index="index"
                  open="(" close=")" separator=",">
                  #{hpzl}
                </foreach>
			</if>
			<if test="hphm!=null and hphm!=''">
				and hphm like #{hphm}
			</if>
			<if test="syr!=null and syr!=''">
				and syr like #{syr}
			</if>
			<if test="lxdh!=null and lxdh!=''">
				and lxdh like #{lxdh}
			</if>
			<if test="ld!=null and ld!=''">
				and ld like #{ld}
			</if>
			<if test="ssdw!=null and ssdw!=''">
				and ssdw like #{ssdw}
			</if>
			<if test="kssj!=null and kssj !='' and jssj!=null and jssj!='' ">
				and fqrq >=#{kssj} 
				and fqrq <![CDATA[<=]]>#{jssj}
			</if>
			<if test="ddmcsz!=null and ddmcsz!=''">
				and  
	            <foreach collection="ddmcsz" item="ddmcsz" index="index"
                  open="(" close=")" separator="or">
                  ktxdd like #{ddmcsz}
                </foreach>
			</if>
		</where>
		order by id desc
		limit #{page},#{rows}
	</select>
